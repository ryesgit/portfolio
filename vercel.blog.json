{
  "buildCommand": "npm run build:blog",
  "outputDirectory": "dist-blog",
  "rewrites": [
    {
      "source": "/post/:slug",
      "destination": "/blog.html"
    },
    {
      "source": "/admin(.*)",
      "destination": "/blog.html"
    },
    {
      "source": "/(.*)",
      "destination": "/blog.html"
    }
  ]
}